<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:elevation="2dp">

    <Button
        android:id="@+id/button13"
        android:layout_width="300dp"
        android:layout_height="51dp"
        android:background="@color/multipleChoice"
        android:text="בתי ספר"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="85dp" />

    <Button
        android:id="@+id/angry_btn"

        android:layout_width="120dp"
        android:layout_height="45dp"
        android:background="@drawable/button_finish"

        android:shadowColor="#000000"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:text="ראשונים"
        android:textColor="#F7F7F7"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="153dp" />

    <Button
        android:id="@+id/angry_btn2"

        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"

        android:background="@drawable/button_finish"
        android:shadowColor="#000000"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:text="ביאליק"
        android:textColor="#F7F7F7"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/angry_btn"
        tools:layout_editor_absoluteX="204dp" />

    <Button
        android:id="@+id/angry_btn3"

        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginTop="16dp"

        android:background="@drawable/button_finish"
        android:shadowColor="#000000"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:text="רביבים"
        android:textColor="#F7F7F7"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@+id/angry_btn2"
        tools:layout_editor_absoluteX="204dp" />

    <Button
        android:id="@+id/angry_btn4"

        android:layout_width="120dp"
        android:layout_height="45dp"

        android:background="@drawable/button_finish"
        android:shadowColor="#000000"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="450dp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="31dp"
        android:layout_height="29dp"
        android:tint="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="@+id/angry_btn2"
        app:layout_constraintTop_toTopOf="@+id/angry_btn2"
        app:layout_constraintVertical_bias="0.75"
        app:srcCompat="@drawable/checkbox_intermediate"
        tools:layout_editor_absoluteX="101dp" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="31dp"
        android:layout_height="29dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:tint="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="@+id/angry_btn"
        app:layout_constraintTop_toTopOf="@+id/angry_btn"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/checkbox_intermediate"
        tools:layout_editor_absoluteX="101dp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="31dp"
        android:layout_height="29dp"
        android:tint="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="@+id/angry_btn3"
        app:layout_constraintTop_toTopOf="@+id/angry_btn3"
        app:srcCompat="@drawable/checkbox_blank_outline"
        tools:layout_editor_absoluteX="101dp" />

    <Button
        android:id="@+id/button14"
        android:layout_width="300dp"
        android:layout_height="51dp"
        android:background="@color/multipleChoice"
        android:text="גנים"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="345dp" />

    <Button
        android:id="@+id/angry_btn4"

        android:layout_width="120dp"
        android:layout_height="45dp"

        android:background="@drawable/button_finish"
        android:shadowColor="#000000"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:text="המשך"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="450dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="145dp"
        android:layout_height="wrap_content"
        android:background="#fafafa"
        android:text="גדרה"
        android:textAllCaps="true"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/button19"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:text="חולון"
        android:textColor="#828282"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/button21"
        android:layout_width="126dp"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:text="ראשון לציון"
        android:textColor="#828282"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="31dp"
        android:layout_height="34dp"
        android:elevation="2dp"
        app:srcCompat="@drawable/ic_menu_down_black_48dp"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="93dp" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="31dp"
        android:layout_height="34dp"
        android:elevation="2dp"
        app:srcCompat="@drawable/ic_menu_down_black_48dp"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="353dp" />

</android.support.constraint.ConstraintLayout>